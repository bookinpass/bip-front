import { ChangeDetectorRef, ElementRef, EventEmitter, Renderer2 } from '@angular/core';
import { FuiTransition } from '../../transition/internal';
export declare class FuiDimmer extends FuiTransition {
    readonly hasClasses: boolean;
    isDimmedChange: EventEmitter<boolean>;
    isClickable: boolean;
    transition: string;
    transitionDuration: number;
    wrapContent: boolean;
    private _transitionController;
    constructor(renderer: Renderer2, element: ElementRef, changeDetector: ChangeDetectorRef);
    private _isDimmed;
    isDimmed: boolean;
    onClick(): void;
}
