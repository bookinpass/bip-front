import { TemplateRef, ViewContainerRef } from '@angular/core';
import { FuiComponentFactory } from '../../../misc/util/internal';
import { IResultContext } from './search';
export declare class FuiSearchResult<T> {
    componentFactory: FuiComponentFactory;
    readonly hasClasses: boolean;
    value: T;
    query: string;
    formatter: (obj: T, query: string) => string;
    templateSibling: ViewContainerRef;
    constructor(componentFactory: FuiComponentFactory);
    private _template?;
    template: TemplateRef<IResultContext<T>> | undefined;
}
