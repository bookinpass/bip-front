import { ElementRef, ViewContainerRef } from '@angular/core';
import { MessageConfig } from '../classes/message-config';
import { FuiActiveMessage } from '../classes/active-message';
import { FuiComponentFactory } from '../../../misc/util/internal';
import { MessageController } from '../classes/message-controller';
export declare class FuiMessageContainer {
    private _componentFactory;
    private _element;
    containerSibling: ViewContainerRef;
    private _messages;
    private _queue;
    constructor(_componentFactory: FuiComponentFactory, _element: ElementRef);
    controller: MessageController;
    show(config: MessageConfig, maxShown: number, showNewestFirst: boolean): FuiActiveMessage;
    dismissAll(): void;
    private open;
    private queue;
    private onMessageClose;
}
